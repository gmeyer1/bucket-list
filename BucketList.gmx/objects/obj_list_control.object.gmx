    if (i &gt; startCross) {
        var width = string_width(name);
        draw_line_width_colour(
            textX - width / 2,
            textY,
            textX + width / 2 + 4,
            textY,
            lineWidth,
            lineColor,
            lineColor
        );
    }
}

